{
	"name": "rbx-file-handler",
	"version": "0.0.0",
	"description": "Parse Roblox files like .rbxlx and .rbxmx files",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"builddoc": "jsdoc2md Classes/*.mjs Datatypes/*.mjs Enums/*.mjs docs/docs.hbs --helper docs/customBlockHelpers.hbs.cjs -c docs/jsdoc.conf > builds/docs/docs.md",
		"build": "rollup --config",
		"pretest": "npm run build",
		"test": "mocha || true"
	},
	"keywords": [],
	"author": "Errorbot1122",
	"license": "ISC",
	"repository": "https://github.com/ErrorBot1122/Roblox-File-Handler",
	"homepage": "https://github.com/ErrorBot1122/Roblox-File-Handler#readme",
	"dependencies": {
		"config": "^3.3.7",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@babel/core": "^7.17.8",
		"@babel/preset-env": "^7.16.11",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-multi-entry": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"chai": "^4.3.6",
		"handlebars": "^4.7.7",
		"handlebars-helpers": "^0.10.0",
		"jsdoc-to-markdown": "^7.1.1",
		"mocha": "^9.2.1",
		"rollup": "^2.70.0",
		"rollup-plugin-polyfill-node": "^0.9.0",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-uglify": "^6.0.4"
	}
}
